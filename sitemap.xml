<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% assign posts = site.posts}{% for post in posts %}  <url>
    <loc>https://10gadget.com/{{ post.url }}</loc>
    <lastmod>{{ page.date }}</lastmod>
  </url>
{% endfor %}
{% assign pages = site.pages}{% for page in pages %}  <url>
    <loc>https://10gadget.com/{{ page.url }}</loc>
    <lastmod>{{ post.date }}</lastmod>
  </url>
{% endfor %}
</urlset>